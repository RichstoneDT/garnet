<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.richstonedt.garnet.dao.GarMenuDao">

    <select id="queryListParentId" resultType="com.richstonedt.garnet.model.GarMenu">
        SELECT *
        FROM gar_menu
        WHERE parent_id = #{parentId}
        ORDER BY order_num ASC
    </select>

</mapper>